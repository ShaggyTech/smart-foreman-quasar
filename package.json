{
  "name": "smart-foreman-quasar",
  "version": "0.0.1",
  "description": "A smarter way to manage your shop tasks, employees, and customers.",
  "productName": "Smart Foreman",
  "author": "ShaggyTech <github.com/ShaggyTech>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "start": "quasar serve ./dist/pwa",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic"
  },
  "dependencies": {
    "@quasar/extras": "1.9.5",
    "@vue/composition-api": "0.6.7",
    "axios": "0.18.1",
    "core-js": "3.6.5",
    "quasar": "1.14.0"
  },
  "devDependencies": {
    "@quasar/app": "2.1.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "2.0.0",
    "@types/node": "10.17.35",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "3.0.4",
    "eslint-plugin-vue": "6.2.2",
    "workbox-webpack-plugin": "5.1.4",
    "eslint-plugin-jest": "23.20.0",
    "wallaby-vue-compiler": "1.0.6",
    "majestic": "1.7.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
